.post-tags {
  list-style-type: none;
  margin: 0;
  padding: 0;
.post-tag {
  font-size: 40px;
  float: left;
  background-color: #e3e3e3;
  padding: 5px 20px;
  border-radius: 10px;
  @media screen and (max-width: $mobileW) {
    margin-top: 5px;
  }
  margin-right: 10px;
  margin-bottom: 10px;
}
